<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>io.quarkus</groupId>
    <artifactId>quarkus-integration-test-grpc-descriptor-sets-parent</artifactId>
    <version>3.21.0</version>
  </parent>
  <artifactId>quarkus-integration-test-grpc-descriptor-sets-alternate-output-dir</artifactId>
  <version>3.21.0</version>
  <name>Quarkus - Integration Tests - gRPC - Descriptor Sets - Alternate Output Dir</name>
  <description>Tests extracted from project Quarkus - Integration Tests - gRPC - Descriptor Sets - Alternate Output Dir</description>
  <properties>
    <quarkus.native.builder-image.pull>missing</quarkus.native.builder-image.pull>
    <quarkus.build.skip>${skipTests}</quarkus.build.skip>
    <native.surefire.skip>${skipTests}</native.surefire.skip>
    <gpg.skip>true</gpg.skip>
    <maven.deploy.skip>true</maven.deploy.skip>
  </properties>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-surefire-plugin</artifactId>
        <configuration>
          <systemPropertyVariables>
            <build.dir>${project.build.directory}</build.dir>
          </systemPropertyVariables>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
